<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="../css/history.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <script src="../js/sdk.js"></script>
    <!-- call LIFF SDK Which is an executable file of LIFF app -->

    <script>
        $(document).ready(async (e) => {
            liff.init(function (data) {
                var userIdfield = data.context.userId;
                $('#data').load('/liff/gethistory', { userId: userIdfield }) // fecth data to shown on history page
            });
        });

    </script>
</head>

<body>

    <div class="container">
        <div class="row" id="header">
            <div class="col-sm">
                <h1>Attendance <br> Info</h1>
            </div>
        </div>
        <!--  Second Container -->
        <div style="overflow-x:auto;">
            <table class="firstdata" id="data">
            </table>
        </div>
    </div>
</body>

</html>